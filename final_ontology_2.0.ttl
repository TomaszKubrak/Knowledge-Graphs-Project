#################################################################
#    Classes
#################################################################

###  http://example.org/aiAidedBrainstorming#AIOffCondition
brain:AIOffCondition rdf:type owl:Class ;
                     rdfs:subClassOf brain:ExperimentalCondition .


###  http://example.org/aiAidedBrainstorming#AIOnCondition
brain:AIOnCondition rdf:type owl:Class ;
                    rdfs:subClassOf brain:ExperimentalCondition .


###  http://example.org/aiAidedBrainstorming#EstimationAccuracy
brain:EstimationAccuracy rdf:type owl:Class ;
                         rdfs:subClassOf brain:PerformanceMetric ,
                                         ai:Accuracy .


###  http://example.org/aiAidedBrainstorming#ExperimentalCondition
brain:ExperimentalCondition rdf:type owl:Class ;
                            owl:equivalentClass ai:ExperimentalCondition .


###  http://example.org/aiAidedBrainstorming#GuesstimationQuestion
brain:GuesstimationQuestion rdf:type owl:Class .


###  http://example.org/aiAidedBrainstorming#GuesstimationTask
brain:GuesstimationTask rdf:type owl:Class ;
                        rdfs:subClassOf :InteractionTask .


###  http://example.org/aiAidedBrainstorming#LLMBrainstorming
brain:LLMBrainstorming rdf:type owl:Class ;
                       rdfs:subClassOf :ProcessingMethod ;
                       rdfs:comment "An LLM-based process for brainstorming." .


###  http://example.org/aiAidedBrainstorming#Participant
brain:Participant rdf:type owl:Class ;
                  owl:equivalentClass ai:Participant ,
                                      gv:Participant ,
                                      hc:Participant ,
                                      hv:Participant ,
                                      pa:Participant ;
                  rdfs:subClassOf :Human .


###  http://example.org/aiAidedBrainstorming#PerformanceMetric
brain:PerformanceMetric rdf:type owl:Class ;
                        owl:equivalentClass ai:PerformanceMetric ,
                                            gv:EvaluationMetric .


###  http://example.org/aiAidedBrainstorming#SemanticTransformation
brain:SemanticTransformation rdf:type owl:Class ;
                             rdfs:comment "A re-framed or substituted question used in guesstimation." .


###  http://example.org/aiAidedBrainstorming#UserExperienceMetric
brain:UserExperienceMetric rdf:type owl:Class ;
                           rdfs:subClassOf brain:PerformanceMetric ,
                                           gv:UserExperienceScore .


###  http://example.org/aiAssistance#AIModel
ai:AIModel rdf:type owl:Class ;
           owl:equivalentClass gx:Classifier ,
                               hc:AIClassifier ;
           rdfs:subClassOf :ArtificialAgent ;
           rdfs:comment "An AI classifier (e.g., fine-tuned VGG-19) assisting the human in image classification." .


###  http://example.org/aiAssistance#AIOffCondition
ai:AIOffCondition rdf:type owl:Class ;
                  rdfs:subClassOf ai:ExperimentalCondition ;
                  owl:disjointWith ai:AIOnCondition ;
                  rdfs:comment "Experimental condition when participant is not allowed to use AI" .


###  http://example.org/aiAssistance#AIOnCondition
ai:AIOnCondition rdf:type owl:Class ;
                 rdfs:subClassOf ai:ExperimentalCondition ;
                 rdfs:comment "Experimental condition when participant is allowed to use AI" .


###  http://example.org/aiAssistance#Accuracy
ai:Accuracy rdf:type owl:Class ;
