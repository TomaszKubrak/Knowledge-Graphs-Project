<http://example.org/combined_ontology> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://example.org/aiAidedBrainstorming#achievedMetric
brain:achievedMetric rdf:type owl:ObjectProperty ;
                     owl:equivalentProperty ai:achievedMetric ,
                                            gv:achievedResult ,
                                            hc:achievedMetric ;
                     rdfs:range brain:PerformanceMetric .


###  http://example.org/aiAidedBrainstorming#hasCondition
brain:hasCondition rdf:type owl:ObjectProperty ;
                   rdfs:domain brain:GuesstimationTask ;
                   rdfs:range brain:ExperimentalCondition .


###  http://example.org/aiAidedBrainstorming#hasQuestion
brain:hasQuestion rdf:type owl:ObjectProperty ;
                  rdfs:domain brain:GuesstimationTask ;
                  rdfs:range brain:GuesstimationQuestion .


###  http://example.org/aiAidedBrainstorming#hasSemanticTransformation
brain:hasSemanticTransformation rdf:type owl:ObjectProperty ;
                                rdfs:domain brain:GuesstimationQuestion ;
                                rdfs:range brain:SemanticTransformation .


###  http://example.org/aiAidedBrainstorming#involvesParticipant
brain:involvesParticipant rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf brain:involvesParticipant ;
                          rdfs:domain brain:GuesstimationTask ;
                          rdfs:range brain:Participant .


###  http://example.org/aiAidedBrainstorming#usesLLMBrainstorming
brain:usesLLMBrainstorming rdf:type owl:ObjectProperty ;
                           rdfs:domain brain:GuesstimationTask ;
                           rdfs:range brain:LLMBrainstorming .


###  http://example.org/aiAssistance#achievedMetric
ai:achievedMetric rdf:type owl:ObjectProperty ;
                  rdfs:range ai:PerformanceMetric .


###  http://example.org/aiAssistance#hasCondition
ai:hasCondition rdf:type owl:ObjectProperty ;
                rdfs:domain ai:NoisyImageClassificationTask ;
                rdfs:range ai:ExperimentalCondition .


###  http://example.org/aiAssistance#involvesParticipant
ai:involvesParticipant rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf ai:involvesParticipant ;
                       rdfs:domain ai:NoisyImageClassificationTask ;
                       rdfs:range ai:Participant .


###  http://example.org/aiAssistance#usesAIModel
ai:usesAIModel rdf:type owl:ObjectProperty ;
               rdfs:domain ai:NoisyImageClassificationTask ;
               rdfs:range ai:AIModel .


###  http://example.org/combined_ontology/capability
:capability rdf:type owl:ObjectProperty ;
            rdfs:domain :Actor ;
            rdfs:range :Capability .


###  http://example.org/combined_ontology/context
:context rdf:type owl:ObjectProperty ;
         rdfs:domain :Scenario ;
         rdfs:range :Context .


###  http://example.org/combined_ontology/dependsOn
:dependsOn rdf:type owl:ObjectProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain :Scenario ;
           rdfs:range :Scenario ;
           rdfs:comment "Scenarios can depend on each other in a transitive way." .


###  http://example.org/combined_ontology/domain
:domain rdf:type owl:ObjectProperty ;
        rdfs:domain :Scenario ;
        rdfs:range :Domain .


###  http://example.org/combined_ontology/endgoal
:endgoal rdf:type owl:ObjectProperty ;
         rdfs:domain :Scenario ;
         rdfs:range :Endgoal .


###  http://example.org/combined_ontology/extendsMethod
:extendsMethod rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain :ProcessingMethod ;
               rdfs:range :ProcessingMethod ;
               rdfs:comment "If method A extends method B, and method B extends method C, then A extends method C." .


###  http://example.org/combined_ontology/hasEthicalConsideration
:hasEthicalConsideration rdf:type owl:ObjectProperty ;
                         rdfs:domain :Scenario ;
                         rdfs:range :EthicalConsideration .


###  http://example.org/combined_ontology/hasInteraction
:hasInteraction rdf:type owl:ObjectProperty ;
                rdfs:domain :Scenario ;
                rdfs:range :Interaction .


###  http://example.org/combined_ontology/hasOwnContext
:hasOwnContext rdf:type owl:ObjectProperty ,
                        owl:ReflexiveProperty ;
               rdfs:domain :Context ;
               rdfs:range :Context ;
               rdfs:comment "Every Context 'hasOwnContext' for itself." .


###  http://example.org/combined_ontology/hasSubTask
:hasSubTask rdf:type owl:ObjectProperty ,
                     owl:TransitiveProperty ;
            rdfs:domain :InteractionTask ;
            rdfs:range :InteractionTask ;
            rdfs:comment "Task decomposition is transitive: if A hasSubTask B and B hasSubTask C, then A hasSubTask C." .


###  http://example.org/combined_ontology/inScenario
:inScenario rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Actor ;
            rdfs:range :Scenario .


###  http://example.org/combined_ontology/informationMethod
:informationMethod rdf:type owl:ObjectProperty ;
                   rdfs:domain :InformationProcessing ;
                   rdfs:range :ProcessingMethod .


###  http://example.org/combined_ontology/interactingAgent
:interactingAgent rdf:type owl:ObjectProperty ;
                  rdfs:domain :Interaction ;
                  rdfs:range :Actor .


###  http://example.org/combined_ontology/interactionMethod
:interactionMethod rdf:type owl:ObjectProperty ;
                   rdfs:domain :Interaction ;
                   rdfs:range :InteractionMethod .


###  http://example.org/combined_ontology/interactionTask
:interactionTask rdf:type owl:ObjectProperty ;
                 rdfs:domain :Interaction ;
                 rdfs:range :InteractionTask .


###  http://example.org/combined_ontology/isResponsibleFor
:isResponsibleFor rdf:type owl:ObjectProperty ,
                           owl:ReflexiveProperty ;
                  rdfs:domain :Actor ;
                  rdfs:range :Actor ;
                  rdfs:comment "Every Actor is considered at least partially responsible for themselves." .


###  http://example.org/combined_ontology/processingInformation
:processingInformation rdf:type owl:ObjectProperty ;
                       rdfs:domain :Actor ;
                       rdfs:range :InformationProcessing .


###  http://example.org/combined_ontology/processingTask
:processingTask rdf:type owl:ObjectProperty ;
                rdfs:domain :InformationProcessing ;
                rdfs:range :ProcessingTask .


